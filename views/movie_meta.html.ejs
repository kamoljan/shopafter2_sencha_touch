
    <meta property="og:title" content="<%= movie.title %>" />
	<meta property="og:type" content="thewatchlist:movie" />
	<meta property="og:image" content="<%= movie.largeImage %>" />
	<meta property="og:url" content="http://www.watchlistapp.com/graph/movie/<%= movie.rottenId %>" />
	<meta property="og:description" content="<%= movie.criticsConsensus || movie.synopsis %>" />

